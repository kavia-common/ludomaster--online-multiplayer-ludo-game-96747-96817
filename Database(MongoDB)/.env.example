# MongoDB connection for LudoMaster Database
# Request these values to be set in the Backend container .env (do not commit real values)

MONGODB_URI=mongodb+srv://<username>:<password>@<cluster-host>/ludomaster?retryWrites=true&w=majority
MONGODB_DB_NAME=ludomaster
# optional
MONGODB_TLS=true
MONGODB_REPLICA_SET=rs0
